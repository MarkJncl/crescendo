</Description>';
 type 'Submodel-Equation'
  ports
   signal in encLeft;
   signal in encRight;
   signal out servoLeft;
   signal out servoRight;
   signal in lfLeft;
   signal in lfRight;
   signal boolean in lfLeftFailFlag;
   signal boolean in lfRightFailFlag;
   signal in energy_used_left_servo;
   signal in energy_used_right_servo;
 end;
 icon bg bottom
  figures
   rectangle 240 312 272 360 fill 13959039;
  terminals
   encLeft 240 320 fixed;
   encRight 272 320 fixed;
   servoLeft 240 336 fixed;
   servoRight 272 336 fixed;
   lfLeft 240 344 fixed;
   lfRight 272 344 fixed;
   lfLeftFailFlag 240 352 fixed;
   lfRightFailFlag 272 352 fixed;
   energy_used_left_servo 240 328 fixed;
   energy_used_right_servo 272 328 fixed;
 end;
 implementation eq
externals
	// servos
   real global import servo_left;
	real global import servo_right;
	
   // encoders
	real global export encoder_left;
	real global export encoder_right;

	// line-following sensors
  real global export lf [2];
  real global export lf_fail [2];

	// line-following sensor fail flags
	boolean global export lf_left_fail_flag;
	boolean global export lf_right_fail_flag;

	// energy used
	real global export energy_used;

equations
   // send encoder values
   encoder_left = encLeft;
	encoder_right = encRight;
   // send line-following sensor values	
   lf[1] = lfLeft;
   lf[2] = lfRight;
	lf_fail[1] = lfLeftFailFlag;
	lf_fail[2] = lfRightFailFlag;

	// HERE DESTECS/VDM CALCULATES 
   // NEW CONTROL VALUES

   // get new servo values
   servoLeft = servo_left + (0 * lfLeft * encLeft);
   servoRight = servo_right + (0 * lfRight * encRight);

	// compute energy used
	energy_used = energy_used_left_servo + energy_used_right_servo;implementation_end;
specification_end;
			end;
