	id = dll (dll_name, initialize_function_name, path_follow_file_name);
   for i = 0 to sample_matrix_size-1 do
      sample_offsets[i+1] = (i - ((sample_matrix_size-1)/2)) * sample_matrix_scale;
   end;
equations
   for x1 = 1 to sample_matrix_size do
      for y1 = 1 to sample_matrix_size do
         input1[1] = id;
         input1[2] = sensor_position1[2] + sample_offsets[x1];
