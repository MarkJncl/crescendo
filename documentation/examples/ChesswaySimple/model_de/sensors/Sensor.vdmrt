class Sensor is subclass of ISensorReal


operations

-- constructor for Sensor
public Sensor: real ==> Sensor
Sensor(v) ==
	value := v;

-- default constructor for Sensor
public Sensor: () ==> Sensor
Sensor() ==
	Sensor(0.0);

-- get sensor value
public GetValue: () ==> real
GetValue() ==
	return value;

end Sensor