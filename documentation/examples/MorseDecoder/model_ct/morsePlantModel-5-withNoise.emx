    implementation eq
parameters					
	string global filename;
	real global sensorViewWidth; 
	real leftPointWeighting = 0.2;
	real leftInnerPointWeighting = 0.2;
	real centrePointWeighting = 0.2;
	real rightInnerPointWeighting = 0.2;
  real rightPointWeighting = 0.2;

variables
	real leftPointOffset, rightPointOffset;
	real leftInnerPointOffset, rightInnerPointOffset;
	real leftPointReading, centrePointReading, rightPointReading;
	real leftInnerPointReading, rightInnerPointReading;

initialequations
	leftPointOffset = sensorViewWidth / 2;
	leftInnerPointOffset = sensorViewWidth / 4;
	rightPointOffset = sensorViewWidth / 2;
	rightInnerPointOffset = sensorViewWidth / 4;

equations					
	leftPointReading = table (filename, sensorCentrePosition - leftPointOffset);
	leftInnerPointReading = table (filename, sensorCentrePosition - leftInnerPointOffset);
	centrePointReading = table (filename, sensorCentrePosition);
	rightInnerPointReading = table (filename, sensorCentrePosition + rightInnerPointOffset);
	rightPointReading = table (filename, sensorCentrePosition + rightPointOffset);

	combinedSensorOutput = (leftPointReading * leftPointWeighting) 
										+ (leftInnerPointReading * leftInnerPointWeighting) 
									  + (centrePointReading * centrePointWeighting) 
										+ (rightInnerPointReading * rightInnerPointWeighting) 
                       + (rightPointReading * rightPointWeighting);

   
 implementation_end;
  end;
