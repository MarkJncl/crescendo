</Description>';
    type AD
     ports
      signal in input;
      signal out output;
    end;
    icon bg bottom
     figures
      rectangle 752 232 784 264 fill 15132390;
      spline 758.4 257.3 776 239.7 color 16711680;
      text 'A' 761.1 242.1 color 16711680 14;
      text 'D' 774.4 253.3 color 16711680 14;
    end;
    implementation eq
parameters

	real global AToDResolutionBits;
	real minimum = 0;		// Minimum output value
	real maximum = 1;		// Minimum output value.
	real initial = 0;			// Initial output
	real global AToDNoiseBits;
	integer gaussianNoiseSeed = 1;
variables
	real hold_signal, windowed_signal, quantisized_signal;
	real lsb, half_lsb;
	real maxNoiseContributionToSignal;

equations
	maxNoiseContributionToSignal = (2 ^ AToDNoiseBits) * lsb;

	lsb = (maximum - minimum) / (exp2 (AToDResolutionBits) - 1);
	half_lsb = lsb / 2;
	hold_signal = sample (input,initial) + (maxNoiseContributionToSignal * gauss(1, gaussianNoiseSeed));
	windowed_signal = if hold_signal <= minimum then
		 minimum + half_lsb
	else
		if hold_signal >= maximum then 
			maximum -half_lsb
		else 
			hold_signal
		end
	end;
	quantisized_signal = if windowed_signal < 0.0 then
		windowed_signal - half_lsb
	else
		 windowed_signal + half_lsb
	end;

	output =  quantisized_signal - (quantisized_signal mod lsb);
    implementation_end;
